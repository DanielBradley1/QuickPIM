{
  "name": "QuickPIM",
  "version": "0.1",
  "description": "Quickly view, select and activate multiple PIM roles at once.",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "webRequest"
  ],
  "host_permissions": [
    "https://graph.microsoft.com/*",
    "https://*.microsoft.com/*",
    "https://*.microsoftonline.com/*",
    "https://*.windows.net/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
